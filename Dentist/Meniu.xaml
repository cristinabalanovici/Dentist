    x:Class="Dentist.Meniu">

    <ContentPage.Content>
        <StackLayout>
            <Button Text="Cabinete" TextColor="Navy" BackgroundColor="AliceBlue" Clicked="Cabinete_OnClicked" HeightRequest="100" FontSize="Medium">
            </Button>
            <Button Text="Clienti" TextColor="Navy" BackgroundColor="AliceBlue" Clicked="Clienti_OnClicked" HeightRequest="100" FontSize="Medium">
